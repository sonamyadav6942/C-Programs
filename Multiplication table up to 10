// C++ program to print Multiplication table up to 10

#include<iostream>
using namespace std;
int main()
{
	int num, i;
	cout << "Enter a number which you want to display table : ";
	cin >> num;
	for(i = 1; i <= 10; i++)
	cout << num << " * " << i << " = " << num * i << endl;
	return 0;
}

